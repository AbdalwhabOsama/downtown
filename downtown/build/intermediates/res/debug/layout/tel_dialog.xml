<?xml version="1.0" encoding="utf-8"?>    
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"    
    android:layout_height="fill_parent"    
   	android:background="@drawable/gradient_bg_intro" >

<LinearLayout    
	android:layout_width="match_parent"    
    android:layout_height="wrap_content"
    android:orientation="vertical">
   	
    <RelativeLayout
	android:layout_width="fill_parent"    
    android:layout_height="45dp"
    android:background="@drawable/actionbar_background"        
    >       
		 <TextView
		   android:id="@+id/dialog_title"
		   android:layout_width="wrap_content"
		   android:layout_height="wrap_content"
		   android:layout_centerVertical="true"
		   android:layout_centerHorizontal="true"
	       android:fontFamily="sans"
	       android:layout_marginTop="1dip"
	       android:textSize="18sp"
	       android:textStyle="bold"
	       android:textColor="#f3f3f3"
	       android:text="@string/test"
			android:paddingLeft="3sp"	 
	  		android:paddingRight="3sp" 
		    />        
    </RelativeLayout>
    <RelativeLayout
	android:layout_width="fill_parent"    
    android:layout_height="320dp"
	android:layout_marginTop="10dp"
	android:layout_marginBottom="10dp"
	android:layout_marginRight="10dp"
	android:layout_marginLeft="20dp" >   	        
        
        <ImageView
            android:id="@+id/logo"
            android:layout_width="25dip"
            android:layout_height="25dip"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:contentDescription="@string/img_desc"            
            android:src="@drawable/logobackface"            
            />
        
		 <TextView
		   android:id="@+id/title_tel"
		   android:layout_width="wrap_content"
		   android:layout_height="wrap_content"		   
		   android:layout_toRightOf="@+id/logo"
	       android:fontFamily="sans-serif"
	       android:layout_marginTop="1dip"
	       android:textSize="16sp"	       
	       android:textColor="#1e90ff"
	       android:text="@string/tels_label"
			android:paddingLeft="3sp"	 
	  		android:paddingRight="3sp" 
		    />
		 
		 <RelativeLayout
		     android:id="@+id/tel1_layout"
		   	android:layout_width="fill_parent"    
    		android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/title_tel"    		 
            >
			  <ImageView
	            android:id="@+id/logo_tel1"
	            android:layout_width="55dp"
	            android:layout_height="55dp"            
	            android:layout_alignParentLeft="true"
	            android:contentDescription="@string/img_desc"            
	            android:src="@drawable/icophone"            
	            />
	        
			 <TextView
			   android:id="@+id/tel1"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"		   
			   android:layout_toRightOf="@+id/logo_tel1"
				android:layout_centerVertical="true"
		       android:fontFamily="sans-serif"
		       android:layout_marginTop="1dip"
		       android:textSize="18sp"	       
		       android:textColor="#343434"
		       android:text="@string/empty"
				android:paddingLeft="3sp"	 
		  		android:paddingRight="3sp" 
			    />
		 </RelativeLayout>
		 <RelativeLayout
		     android:id="@+id/tel2_layout"
		   	android:layout_width="fill_parent"    
    		android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/tel1_layout"    		 
            >
			  <ImageView
	            android:id="@+id/logo_tel2"
	            android:layout_width="55dp"
	            android:layout_height="55dp"            
	            android:layout_alignParentLeft="true"
	            android:contentDescription="@string/img_desc"            
	            android:src="@drawable/icophone"            
	            />
	        
			 <TextView
			   android:id="@+id/tel2"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"		   
			   android:layout_toRightOf="@+id/logo_tel2"
				android:layout_centerVertical="true"
		       android:fontFamily="sans-serif"
		       android:layout_marginTop="1dip"
		       android:textSize="18sp"	       
		       android:textColor="#343434"
		       android:text="@string/empty"
				android:paddingLeft="3sp"	 
		  		android:paddingRight="3sp" 
			    />
		 </RelativeLayout>
		 <RelativeLayout
		     android:id="@+id/tel3_layout"
		   	android:layout_width="fill_parent"    
    		android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/tel2_layout"    		 
            >
			  <ImageView
	            android:id="@+id/logo_tel3"
	            android:layout_width="55dp"
	            android:layout_height="55dp"            
	            android:layout_alignParentLeft="true"
	            android:contentDescription="@string/img_desc"            
	            android:src="@drawable/icophone"            
	            />
	        
			 <TextView
			   android:id="@+id/tel3"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"		   
			   android:layout_toRightOf="@+id/logo_tel3"
				android:layout_centerVertical="true"
		       android:fontFamily="sans-serif"
		       android:layout_marginTop="1dip"
		       android:textSize="18sp"	       
		       android:textColor="#343434"
		       android:text="@string/empty"
				android:paddingLeft="3sp"	 
		  		android:paddingRight="3sp" 
			    />
		 </RelativeLayout>
		 
	    <View
	    android:id="@+id/horizon_line0"
	    android:layout_width="fill_parent"
	    android:layout_height="1dp"
	    android:paddingBottom="1sp"
	    android:layout_below="@+id/tel3_layout"
	    android:layout_marginTop="5dp"
	    android:layout_marginBottom="5dp"
	    android:background="@color/actionbar_background_start"/>
	     		 
        <ImageView
            android:id="@+id/logo2"
            android:layout_width="25dip"
            android:layout_height="25dip"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/horizon_line0"
            android:contentDescription="@string/img_desc"            
            android:src="@drawable/logobackface"            
            />              
        
		 <TextView
		   android:id="@+id/title_fax"
		   android:layout_width="wrap_content"
		   android:layout_height="wrap_content"
		   android:layout_below="@+id/horizon_line0"					   
		   android:layout_toRightOf="@+id/logo2"
	       android:fontFamily="sans-serif"
	       android:layout_marginTop="1dip"
	       android:textSize="16sp"	       
	       android:textColor="#1e90ff"
	       android:text="@string/fax_label"
			android:paddingLeft="3sp"	 
	  		android:paddingRight="3sp" 
		    />
		 <RelativeLayout
		     android:id="@+id/fax_layout"
		   	android:layout_width="fill_parent"    
    		android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/logo2"    		 
            >
			  <ImageView
	            android:id="@+id/logo_fax"
	            android:layout_width="55dp"
	            android:layout_height="55dp"            
	            android:layout_alignParentLeft="true"
	            android:contentDescription="@string/img_desc"            
	            android:src="@drawable/icophone"            
	            />
	        
			 <TextView
			   android:id="@+id/fax"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"		   
			   android:layout_toRightOf="@+id/logo_fax"
				android:layout_centerVertical="true"
		       android:fontFamily="sans-serif"
		       android:layout_marginTop="1dip"
		       android:textSize="18sp"	       
		       android:textColor="#343434"
		       android:text="@string/empty"
				android:paddingLeft="3sp"	 
		  		android:paddingRight="3sp" 
			    />
		 </RelativeLayout>
		 
	    <View
	    android:id="@+id/horizon_line"
	    android:layout_width="fill_parent"
	    android:layout_height="1dp"
	    android:layout_below="@+id/fax_layout"
	    android:paddingBottom="1sp"
	    android:layout_marginTop="5dp"	    
	    android:background="@color/actionbar_background_start"/> 		 	    		    		 
	</RelativeLayout>  	
    
		    <Button
    	    android:id="@+id/btn_close"
    	   	android:layout_width="fill_parent"
		   	android:layout_height="40dp"
    	    android:text="@string/close"    	        	   
    	  	android:textSize="18sp"
	       	android:textStyle="normal"
	       	android:textColor="#f3f3f3"
    	    android:padding="3dip"
    	    android:background="@drawable/actionbar_btn"    	    
        	/>
</LinearLayout>
</ScrollView>


